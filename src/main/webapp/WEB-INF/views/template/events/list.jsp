<h5>
  Events
  <button class="fa fa-plus" type="button" data-toggle="modal" data-target="#eventModal"></button>
</h5>
<ul id="events" class="list-group list-group-flush">
  <li class="list-group-item">
    <input type="text" readonly="readonly" min="0" form="form" class="form-control" id="month" name="eventMonth">
    <select name="eventType" form="form" class="form-control" id="eventType">
      <option value="" selected="selected"></option>
    </select>
    <input type="text" readonly="readonly" form="form" class="form-control" id="value"
      name="eventValue"
    >
    <span class="badge badge-pill badge-light"></span>
    <span class="pull-right"></span>
  </li>
</ul>
