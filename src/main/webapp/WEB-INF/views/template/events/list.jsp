<h5 align="center">Events</h5>
<table class="table table-hover">
  <thead>
    <tr>
      <th>month</th>
      <th>type</th>
      <th>value</th>
    </tr>
  </thead>
  <tbody id="events">
    <tr class="event" data-toggle="modal" data-target="#eventModal">
      <td><input type="hidden" readonly="readonly" form="form" id="month" name="eventMonth"></td>
      <td><input type="hidden" readonly="readonly" name="eventType" form="form" id="eventType"></td>
      <td><input type="hidden" readonly="readonly" form="form" id="value" name="eventValue"></td>
    </tr>
  </tbody>
</table>
